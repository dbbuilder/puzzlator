{
  "name": "puzzler",
  "version": "0.1.0",
  "private": true,
  "description": "AI-powered puzzle game generator with dynamic challenges and interactive graphics",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:e2e": "cypress run",
    "test:e2e:dev": "cypress open",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:reset": "supabase db reset",
    "db:migrate": "supabase migration up",
    "db:types": "supabase gen types typescript --local > src/types/database.generated.ts",
    "db:seed": "node scripts/seed-database.js",
    "setup:supabase": "bash scripts/setup-supabase.sh",
    "setup:complete": "bash scripts/init-database.sh",
    "postinstall": "echo 'Run npm run setup:complete to set up the database'"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.7",
    "@types/uuid": "^10.0.0",
    "@vueuse/core": "^10.7.2",
    "lucide-vue-next": "^0.309.0",
    "openai": "^4.24.7",
    "phaser": "^3.70.0",
    "pinia": "^2.1.7",
    "uuid": "^11.1.0",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5",
    "vue-toastification": "^2.0.0-rc.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.6.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^18.19.10",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.17",
    "cypress": "^13.6.4",
    "eslint": "^8.56.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-vue": "^9.20.1",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.3.0",
    "vite": "^5.0.11",
    "vitest": "^1.2.1",
    "vue-tsc": "^1.8.27"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dbbuilder/puzzler.git"
  },
  "keywords": [
    "puzzle",
    "game",
    "ai",
    "vue",
    "typescript",
    "supabase",
    "phaser"
  ],
  "author": "dbbuilder",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dbbuilder/puzzler/issues"
  },
  "homepage": "https://github.com/dbbuilder/puzzler#readme"
}
